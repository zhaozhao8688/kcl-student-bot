# SerpAPI (Google Search)
SERPAPI_API_KEY=your_serpapi_key_here

# Firecrawl (Web Scraping)
FIRECRAWL_API_KEY=your_firecrawl_key_here

# Optional: Session Configuration
SESSION_MAX_AGE_HOURS=24
