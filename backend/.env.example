# Backend Environment Variables Example

# OpenRouter API
OPENROUTER_API_KEY=your_openrouter_api_key_here
DEFAULT_MODEL=anthropic/claude-3.5-sonnet
# Options: anthropic/claude-opus-4.5, anthropic/claude-3.5-sonnet,
#          anthropic/claude-3.5-haiku, google/gemini-3-flash-preview,
#          openai/gpt-5, openai/gpt-4o, etc.

# Supabase
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_key_here

# SerpAPI (Google Search)
SERPAPI_API_KEY=your_serpapi_key_here

# Firecrawl (Web Scraping)
FIRECRAWL_API_KEY=your_firecrawl_key_here

# Optional: Session Configuration
SESSION_MAX_AGE_HOURS=24

# Agent Configuration
MAX_AGENT_ITERATIONS=5    # Max reasoning loops (default: 5)
ENABLE_PLANNING=false     # Enable planning step (default: false)
